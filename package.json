{
  "name": "raydium-launchpad-monitor",
  "version": "1.0.0",
  "description": "Monitor Raydium Launchpad for new token mints",
  "main": "raydium-launchpad-monitor-new-token-mint.ts",
  "type": "commonjs",
  "scripts": {
    "rlmonitor:mint": "npx ts-node src/monitors/raydium-launchpad/raydium-launchpad-monitor-new-token-mint.ts",
    "rlmonitor:trans": "npx ts-node src/monitors/raydium-launchpad/raydium-launchpad-transaction-monitor.ts",
    "rlmonitor:account": "npx ts-node src/monitors/raydium-launchpad/raydium-launchpad-account-monitor.ts",
    "pfmonitor:mint": "npx ts-node src/monitors/pumpfun/pumpfun-monitor-new-token-mint.ts",
    "pfmonitor:account": "npx ts-node src/monitors/pumpfun/pump-fun-monitor-account.ts",
    "pfmonitor:transaction": "npx ts-node src/monitors/pumpfun/pumpfun-monitor-transaction.ts",
    "pfmonitor:price": "npx ts-node src/monitors/pumpfun/pumpfun-monitor-token-price.ts",
    "graduation:monitor": "npx ts-node src/monitors/graduation/graduation-monitor.ts",
    "build": "npx tsc",
    "ui:export": "npx tsx ui-viewer/scripts/export-data.ts",
    "ui:open": "start ui-viewer/index.html",
    "ui:serve": "cd ui-viewer && npx http-server . -p 8888 -c-1 --cors",
    "db:setup": "npx ts-node src/database/setup.ts",
    "db:setup:pools": "npx ts-node src/database/test-pool-operations.ts setup",
    "db:test:pools": "npx ts-node src/database/test-pool-operations.ts test",
    "db:setup:transactions": "npx ts-node src/database/test-transaction-operations.ts setup",
    "db:test:transactions": "npx ts-node src/database/test-transaction-operations.ts test",
    "db:perf:transactions": "npx ts-node src/database/test-transaction-operations.ts perf",
    "db:setup:prices": "npx ts-node src/database/test-price-aggregates.ts setup",
    "db:test:prices": "npx ts-node src/database/test-price-aggregates.ts test",
    "db:validate": "npx ts-node src/database/validate-tokens.ts",
    "check-tx": "npx ts-node src/utils/transaction-lookup.ts",
    "db:check:price": "npx ts-node src/database/check-price-monitor-data.ts",
    "db:migrate:price": "npx ts-node src/database/run-price-migration.ts"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@raydium-io/raydium-sdk": "^1.3.1-beta.58",
    "@shyft-to/solana-transaction-parser": "^2.0.0",
    "@solana/web3.js": "^1.93.0",
    "@triton-one/yellowstone-grpc": "^4.0.0",
    "@types/bs58": "^4.0.4",
    "@types/lodash": "^4.17.20",
    "bs58": "^6.0.0",
    "dotenv": "^16.4.7",
    "lodash": "^4.17.21",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/bn.js": "^5.2.0",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
