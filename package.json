{
  "name": "raydium-launchpad-monitor",
  "version": "1.0.0",
  "description": "Monitor Raydium Launchpad for new token mints",
  "main": "raydium-launchpad-monitor-new-token-mint.ts",
  "type": "commonjs",
  "scripts": {
    "rlmonitor:mint": "npx ts-node src/monitors/raydium-launchpad/raydium-launchpad-monitor-new-token-mint.ts",
    "rlmonitor:trans": "npx ts-node src/monitors/raydium-launchpad/raydium-launchpad-transaction-monitor.ts",
    "rlmonitor:account": "npx ts-node src/monitors/raydium-launchpad/raydium-launchpad-account-monitor.ts",
    "rlmonitor:account:v2": "npx ts-node src/monitors/raydium-launchpad/raydium-launchpad-account-monitor-v2.ts",
    "rlmonitor:account:v3": "npx ts-node src/monitors/raydium-launchpad/raydium-launchpad-account-monitor-v3.ts",
    "pfmonitor:mint": "npx ts-node src/monitors/pumpfun/pumpfun-monitor-new-token-mint.ts",
    "pfmonitor:account": "npx ts-node src/monitors/pumpfun/pump-fun-monitor-account.ts",
    "pfmonitor:transaction": "npx ts-node src/monitors/pumpfun/pumpfun-monitor-transaction.ts",
    "pfmonitor:price": "npx ts-node src/monitors/pumpfun/pumpfun-monitor-token-price.ts",
    "graduation:monitor": "npx ts-node src/monitors/graduation/graduation-monitor.ts",
    "build": "npx tsc",
    "sol-price:updater": "npx ts-node src/services/price-v2/price-updater.ts",
    "sol-price:migrate": "npx ts-node src/services/price-v2/migrate-to-v2.ts",
    "dashboard:serve": "npx ts-node src/api/server.ts",
    "dashboard:serve:dev": "node scripts/start-dashboard-dev.js",
    "score:monitor": "npx ts-node src/monitors/pumpfun/technical-score-monitor.ts",
    "holder:monitor": "npx ts-node src/monitors/holder-score-monitor.ts",
    "holder:smart": "npx ts-node src/monitors/holder-score-smart-monitor.ts",
    "test:holder": "npx ts-node src/test/test-holder-score.ts",
    "db:migrate:holder": "psql -U postgres -d megatron_v2 -f src/database/migrations/014_create_holder_tracking_tables.sql"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@pythnetwork/client": "^2.22.1",
    "@pythnetwork/hermes-client": "^2.0.0",
    "@raydium-io/raydium-sdk": "^1.3.1-beta.58",
    "@shyft-to/solana-transaction-parser": "^2.0.0",
    "@solana/web3.js": "^1.93.0",
    "@triton-one/yellowstone-grpc": "^4.0.0",
    "@types/bs58": "^4.0.4",
    "@types/lodash": "^4.17.20",
    "bs58": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "lodash": "^4.17.21",
    "pg": "^8.11.3",
    "axios": "^1.6.0",
    "decimal.js": "^10.4.3",
    "helius-sdk": "^1.5.1"
  },
  "devDependencies": {
    "@types/bn.js": "^5.2.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
